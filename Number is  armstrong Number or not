#include<stdio.h>

#include<math.h>

int main(){

    //only for three digits

   int n,x,y,z,m;

   printf("enter no. : ");
   scanf("%d" ,&n);

   z = n % 10;
   m = n / 10;
   y = m % 10;
   x = m / 10;

   if(n == x*x*x + y*y*y + z*z*z){

    printf("%d is armstrong number" ,n);
   }

   else{
    printf("%d is not armstrong number" ,n);
   }

   

   
    return 0;
}
   
